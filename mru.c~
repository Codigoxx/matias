#include <stdio.h>


float mru (int velocidad , int distancia , int tiempo ) {

	if ( velocidad , distancia ) { 

		else velocidad / distancia = tiempo ;
	}

	if ( distancia , tiempo ) {

		else distancia / tiempo = velocidad ;

	}

	if ( tiempo , velocidad ) {

		else velocidad * tiempo = distancia ;
	}


	int main () {

		velocidad (6);
		distancia (80);
		tiempo (0);
		return 0 ;
	}

	printf ( Velocidad , " Velocidad \n " ) ;

	printf ( distancia , " distancia \n " ) ;

	printf ( tiempo , " tiempo \n " ) ;
}



